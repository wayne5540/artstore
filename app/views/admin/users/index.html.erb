<h1>會員列表</h1>
<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>會員名稱</th>
      <th>Email</th>
      <th>建立時間</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= render_user_name(user)%></td>
        <td><%= render_user_email(user) %></td>
        <td><%= render_created_time(user) %></td>
        <td><%= render_edit_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
